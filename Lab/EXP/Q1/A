
class Animal {
    void eat() {
        System.out.println("Animal eats food");
    }
}


interface Pet {
    void play();
}

interface Trainable {
    void train();
}


class Dog extends Animal implements Pet, Trainable {

    @Override
    public void play() {
        System.out.println("Dog loves to play");
    }

    @Override
    public void train() {
        System.out.println("Dog is being trained");
    }
}

public class TestClassExtension {
    public static void main(String[] args) {
    System.out.println("this is poorab jain,24csu151");
        Dog myDog = new Dog();
        myDog.eat();     
        myDog.play();    
        myDog.train();   
    }
}
